{
    "compilerOptions": {
        "baseUrl": "src",
        "target": "es5",
        "types": [
            "node", "jest"
        ],
        "lib": [
            "dom", "dom.iterable", "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        ".jest/setup.ts",
        "**/*.ts",
        "**/*.tsx"
, "src/templates/Places/styles.js"    ],
    "exclude": ["node_modules"]
}